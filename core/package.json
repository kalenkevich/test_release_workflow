{
  "name": "@kalenkevich/test_release_workflow",
  "version": "0.1.1",
  "description": "",
  "type": "module",
  "typings": "./dist/types/main.d.ts",
  "main": "./dist/cjs/main.js",
  "module": "./dist/esm/main.js",
  "exports": {
    ".": {
      "types": "./dist/types/main.d.ts",
      "import": "./dist/esm/main.js",
      "require": "./dist/cjs/main.js",
      "default": "./dist/esm/main.js"
    }
  },
  "scripts": {
    "build": "npm run build:types && npm run build:cjs && npm run build:esm",
    "build:types": "tsc --emitDeclarationOnly",
    "build:cjs": "esbuild src/main.ts --bundle --platform=node --target=node20 --outdir=dist/cjs --format=cjs --sourcemap",
    "build:esm": "esbuild src/main.ts --bundle --platform=node --target=node20 --outdir=dist/esm --format=esm --sourcemap"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kalenkevich/test_release_workflow.git"
  },
