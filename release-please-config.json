{
  "packages": {
    "core": {
      "changelog-path": "CHANGELOG.md",
      "component": "test_release_workflow",
      "release-type": "node",
      "pull-request-header": "Automated Release PR",
      "pull-request-title-pattern": "release: ${version}",
      "extra-files": [
        {
          "path": "src/version.ts",
          "type": "generic"
        }
      ]
    },
    "dev": {
      "changelog-path": "CHANGELOG.md",
      "component": "test_release_workflow-dev",
      "release-type": "node",
      "pull-request-header": "Automated Release PR",
      "pull-request-title-pattern": "release: ${version}",
      "extra-files": [
        {
          "path": "src/version.ts",
          "type": "generic"
        }
      ]
    }
  },
  "plugins": [
    {
      "type": "node-workspace",
      "merge": false
    },
    {
      "type": "linked-versions",
      "groupName": "adk",
      "components": [
        "test_release_workflow", "test_release_workflow-dev"
      ]
    }
  ],
  "extra-files": [
    {
      "path": "package.json",
      "type": "json",
      "jsonpath": "$.version"
    }
  ],
  "$schema": "https://raw.githubusercontent.com/googleapis/release-please/main/schemas/config.json"
}
